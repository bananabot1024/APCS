/**
 * 
 * @author Michelle Li
 * Period 1
 *
 */
public class Fraction {

	private int numerator, denominator;
	
	public Fraction() {
		numerator = 0;
		denominator = 1;
	}
	
	public Fraction(int n) {
		numerator = n;
		denominator = 1;
	}
	
	public Fraction(int n, int d) {
		numerator = n;
		denominator = d;
	}
	
	

}
